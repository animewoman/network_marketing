{
    port: 7777,
    host: "0.0.0.0",

    endpoints: [
        {
            method: "get",
            url: "/api/user/get",
            func: "getUserId"
        },
        /*
                    url: "/api/user/delete",
                    body params:
                        _id: "asdfsadfsf"
                    
                    response: status 500 || 200
        */
        {
            method: "post", 
            url: "/api/user/delete",
            func: "deleteUser",
            authenticated: true,
            isAdmin: true
        },
        /*
                    url: "/api/user/create",
                    body params:
                        login.
                        password,
                        email,
                        phone
                    
                    response: status 400(user exists) ||
                                500 (server problems) ||
                                201 (OK, user created)
        */
        {
            method: "post",
            url: "/api/user/create",
            func: "createUser"
        },
        
        {
            method: "get",
            url: "/api/user/profile",
            func: "getProfile",
            authenticated: true
        },
        {
            method: "post",
            url: "/api/auth/login",
